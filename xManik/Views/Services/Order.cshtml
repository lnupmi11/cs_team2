@model RegisterOrderViewModel
@{
    ViewData["Title"] = "MakeOrder";
}

<h2>MakeOrder</h2>

<div class="row">
    <div class="col-md-4">
        <form  method="post">
            <div class="form-group">
                <label asp-for="ServiceTime"></label>
                <input asp-for="ServiceTime" class="form-control" />
                <span asp-validation-for="ServiceTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="OrderDetails"></label>
                <input asp-for="OrderDetails" class="form-control" />
                <span asp-validation-for="OrderDetails" class="text-danger"></span>
            </div>
            <input asp-for="ServiceId" value="@ViewBag.ServiceId" class="form-control hidden" />
            <input asp-for="UserId" value="@ViewBag.UserId" class="form-control hidden" />
            <button type="submit" class="btn btn-default">Замовити</button>
        </form>
        @ViewBag.token
    </div>
</div>